#include <stdio.h>

/**
 *main - Entry point
 *Return: Always 0
 */

int main(void)
{
	char lowercase = 'a';
	char uppercase = 'A';

	while (lower <= 'z')
	{
		putchar(lowercase);
		lowercase++;
	}

	while (uppercase <= 'Z')
	{
		putchar(uppercase);
		uppercase++;
	}

	putchar('\n');

